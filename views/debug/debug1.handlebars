<style>
  body {
    background: url(/img/dashbg.jpg);
  }
</style>
<body style="background-color: #737373">
    <nav class="navbar navbar-expand-sm bg-dark navbar-dark">
      <a class="navbar-brand" href="/dashboard">
        <img src="/img/logowhite.png" alt="logo">
      </a>
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <a class="nav-link" href="/search/teams">Find Team</a>
        </li>
        <li class="nav-item active">
          <a class="nav-link" href="../dashboard">Dashboard</a>
        </li>
      </ul>
      <ul class="navbar-nav">
        <li class="nav-item active">
          <a class="nav-link" href="/debug/debug1">MyTeams</a>
        </li>
        <li class="nav-item active">
          <a class="nav-link" href="#">Logout</a>
        </li>
      </ul>
    </nav>
    <div class="container">
      <div class="row-md-12">
        <div class="col-md-12 mt-3">
          <h2 class="font-weight-bold"style="text-align: center; color: white;">MY TEAMS</h2>
        </div>
      </div>
    </div>
    <br><br>
    <div class="container border border-white text-white bg-dark">
        <div class="row">
            <div class="col text-left font-weight-bold">
                <p>#LIST_TEAM</p>
                <a href="#" class="btn btn-primary">View</a>
            </div>
        </div>
    </div>
    <div class="list-group">
      {{#if infos}}
      <div class="card w-75 mx-auto">
        <div class="card-body">
          <h5 class="card-title text-center">{{infos}}</h5>
        </div>
        <a href="#" class="list-group-item list-group-item-action active">
          {{infos}}
        </a>
        {{else}}
        {{#each teams}}
        <div class="card w-75 mx-auto">
          <div class="card-body">
            <h5 class="card-title">Team {{this.name}}</h5>
            <p class="card-text"><samp>{{this}}</samp></p>
            <a href="#" class="btn btn-primary">Join</a>
          </div>
        </div>
        {{/each}}
        {{/if}}
      </div>
  
    </div>
  </body>
  
  <script>
    $(document).ready(function () {
      console.log($("#gameFilter option:selected"));
      console.log($("#tipeFilter option:selected"))
      console.log($("#gameFilter option:selected"))
      $('input[name="noofppl"]').keyup(function () {
        console.log($(this).val());
      });
    });
  </script>
  
  <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content bg-dark" style="color: white">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Create Team</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="color: white">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form>
            <div class="form-group">
              <label for="Teamname">Team Name</label>
              <input type="text" class="form-control" id="tname" placeholder="Team Name">
            </div>
            <label class="radio-inline"><input type="radio" name="optradio"> Public</label>
            <label class="radio-inline"><input type="radio" name="optradio"> Private</label>
            <div class="form-group">
              <label for="exampleFormControlTextarea1">Deskripsi</label>
              <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="Deskripsi"></textarea>
            </div>
  
        </div>
        <div class="modal-footer text-center">
          <button type="submit" class="btn btn-primary" style="width: 100%">Submit</button>
        </div>
        </form>
      </div>
    </div>
  </div>
  
  </body>