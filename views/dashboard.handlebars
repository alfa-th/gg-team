  <br><br><br>
  {{#if username}}
  <center>
    <h3 class="text-white font-weight-bold">SALAM SEJAHTERA {{username}}</h3>
  </center>
  {{/if}}
  <br>
  <div class="container border border-white text-white bg-dark">
    <div class="row">
      <div class="col text-center font-weight-bold">
        <p style="margin-top: 16px; margin-bottom: 0px;">GAME</p>
        <p>LIST</p>
      </div>
      <div class="col">
        <a href="#">
          <img id="logo-dota"src="/img/dota2.png" alt="dotka2" style="width: 20%;">
        </a>
      </div>
      <div class="col">
        <a href="#">
          <img id="logo-pubg"src="/img/pubg.png" alt="pabji" style="width: 20%;">
        </a>
      </div>
    </div>
  </div>
  <br>
  <div class="container text-center text-white">
    <div class="row">
      <div class="col-md-4 bg-dark" style="margin-left: 15px; padding: 0%;">
        <p></p>
        <img src="/img/profile-logo.png" alt="just-logo" style="width: 10%;">
        <h3>#player_name</h3>
        <p>#rank_player</p>
        <p>_____________________________________________</p>
        <div class="w-100"></div>
        <h4>Joined Team</h3>
        <div class="row">
          <div class="col-md">
            <p>#nama_team</p>
          </div>
          <div class="col-md ml-auto">
            <p>#role_team</p>
          </div>
        </div>
      </div>
      <div class="col-md-7 ml-auto bg-dark" style="margin-right: 15px;">
        <h3 class="font-weight-bold">Team Message</h3>
        <div class="col-md">
          <p class="text-left">#captain_name : #message</p>
        </div>
        {{!-- <div class="col-md" style="background-color: black;">
